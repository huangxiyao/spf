<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="com.hp.spp.eservice.StandardParameterSet" table="SPP_STANDARD_PARAMETERSET">
		<id name="id" column="ID" type="long">
			<generator class="sequence">
				<param name="sequence">SPP_STANDARD_PARAMETERSET_SEQ</param>
			</generator>
		</id>
		<property name="name" column="NAME" type="string" length="100"/>
		<property name="creationDate" column="CREATION_DATE" type="date" />
		<property name="lastModificationDate" column="LAST_MODIFICATION_DATE" type="date" />
		<many-to-one name="site" class="com.hp.spp.eservice.Site" column="SITE_ID"/>
		<set name="parameterList" inverse="true" lazy="true" cascade="all">
			<key column="STANDARD_PARAMETERSET_ID"/>
			<one-to-many class="com.hp.spp.eservice.Parameter"/>
		</set>
	</class>
</hibernate-mapping>
	