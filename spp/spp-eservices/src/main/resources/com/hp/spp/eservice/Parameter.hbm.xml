<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="com.hp.spp.eservice.Parameter" table="SPP_ESERVICE_PARAMETER">
		<id name="id" column="ID" type="long">
			<generator class="sequence">
				<param name="sequence">SPP_ESERVICE_PARAMETER_ID_SEQ</param>
			</generator>
		</id>
		<property name="name" column="NAME" type="string" length="100"/>
		<property name="expression" column="EXPRESSION" type="string" length="500"/>
		<many-to-one name="EService" class="com.hp.spp.eservice.EService" column="ESERVICE_ID"/>
		<many-to-one name="standardParameterSet" class="com.hp.spp.eservice.StandardParameterSet" column="STANDARD_PARAMETERSET_ID"/>
</class>
</hibernate-mapping>
	