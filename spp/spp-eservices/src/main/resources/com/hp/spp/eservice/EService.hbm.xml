<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="com.hp.spp.eservice.EService" table="SPP_ESERVICE">
	<id name="id" column="ID" type="long">
		<generator class="sequence">
				<param name="sequence">SPP_ESERVICE_ID_SEQ</param>
		</generator>
	</id>
			<property name="name" column="NAME" type="string" length="100"/>
			<property name="method" column="METHOD" type="string" length="100"/>
			<property name="productionUrl" column="PRODUCTION_URL" type="string" length="100"/>
			<property name="testUrl" column="TEST_URL" type="string" length="100"/>
			<property name="creationDate" column="CREATION_DATE" type="date" />
			<property name="lastModificationDate" column="LAST_MODIFICATION_DATE" type="date" />
			<property name="navigationMode" column="IS_NEW_WINDOW" type="long"/>
			<property name="windowParameters" column="WINDOW_PARAMETERS" type="string" length="500"/>
			<property name="characterEncoding" column="CHAR_ENCODING" type="string" length="50"/>
			<property name="securityMode" column="SECURITY_MODE" type="long"/>
		    <property name="simulationMode" column="SIMULATION_MODE" type="long"/>
		
	<many-to-one name="standardParameterSet" class="com.hp.spp.eservice.StandardParameterSet" column="STANDARDPARAMETERSET_ID"/>
		<many-to-one name="site" class="com.hp.spp.eservice.Site" column="SITE_ID" />
		<set name="parameterList" inverse="true" lazy="true" cascade="all">
			<key column="ESERVICE_ID"/>
			<one-to-many class="com.hp.spp.eservice.Parameter"/>
		</set>
	

</class>
</hibernate-mapping>
	