<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<parent>
		<groupId>com.hp.it.spp</groupId>
		<version>4.9.4-SNAPSHOT</version>
		<artifactId>spp</artifactId>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.hp.it.spp</groupId>
	<artifactId>spp-hpputils</artifactId>
	<packaging>jar</packaging>
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.hp.it.spp</groupId>
			<artifactId>spp-config</artifactId>
			<version>${spp.version}</version>
		</dependency>
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.13</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.hp.it.spp</groupId>
			<artifactId>spp-portal-crypto</artifactId>
			<version>${spp.version}</version>
		</dependency>
		<dependency>
			<groupId>com.hp.it.spp</groupId>
			<artifactId>spp-profile</artifactId>
			<version>${spp.version}</version>
		</dependency>
		<dependency>
			<groupId>com.hp.it.spp</groupId>
			<artifactId>spp-portal-common</artifactId>
			<version>${spp.version}</version>
			<exclusions>
				<exclusion>
					<groupId>com.hp.it.spp</groupId>
					<artifactId>spp-common</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>commons-codec</groupId>
			<artifactId>commons-codec</artifactId>
			<version>1.2</version>
		</dependency>
	</dependencies>
	<build>
		<finalName>spp-hpputils</finalName>
		<filters>
			<filter>../filters/${env}.properties</filter>
		</filters>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>


	</build>
</project>
