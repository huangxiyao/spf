<?xml version="1.0" encoding="UTF-8"?>
<Site name="Smart Portal">
	<Group name='GENERIC_GROUP'>
		<And>
			<!-- Blow the attribute should be userStatus, not userstatus -->
			<Equals attribute="userstatus">Active</Equals>
			<Equals attribute="userProgramName">EU-Competitor Supplies</Equals>
			<Or>
				<Equals attribute="userCountryCode">Switzerland</Equals>
				<Equals attribute="userCountryCode">Lichtenstein</Equals>
			</Or>
			<And>
				<InGroup value="GENERIC_GROUP_TEST2"/>
				<Equals attribute="nonexistentattribute">Partner Portal Administrator</Equals>
				<Not>
					<Or>
						<Equals attribute="userCountryCode">UK</Equals>
						<Equals attribute="userCountryCode">IRELAND</Equals>
					</Or>
				</Not>
			</And>
		</And>
	</Group>
	<Group name='GENERIC_GROUP_TEST2'>
		<And>
			<Equals attribute="userStatus">Active</Equals>
			<Equals attribute="userProgramName">EU-Competitor Supplies</Equals>
			<Or>
				<Equals attribute="userCountryCode">Switzerland</Equals>
				<Equals attribute="userCountryCode">Lichtenstein</Equals>
			</Or>
			<And>
				<InGroup value="PARTNER_GROUP_TEST"/>
				<Equals attribute="userRole">Partner Portal Administrator</Equals>
				<Not>
					<Or>
						<Equals attribute="userCountryCode">UK</Equals>
						<Equals attribute="userCountryCode">IRELAND</Equals>
					</Or>
				</Not>
			</And>
		</And>
	</Group>
	<Group name='PARTNER_GROUP_TEST'>
		<And>
			<Equals attribute="userStatus">Active</Equals>
			<Equals attribute="userProgramName">EU-Competitor Supplies</Equals>
			<Or>
				<Equals attribute="userCountryCode">Switzerland</Equals>
				<Equals attribute="userCountryCode">Lichtenstein</Equals>
			</Or>
			<And>
				<Equals attribute="userRole">Partner Portal Administrator</Equals>
				<Not>
					<Or>
						<Equals attribute="userCountryCode">UK</Equals>
						<Equals attribute="userCountryCode">IRELAND</Equals>
					</Or>
				</Not>
			</And>
		</And>
	</Group>
</Site>