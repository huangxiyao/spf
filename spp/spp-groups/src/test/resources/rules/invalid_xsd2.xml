<?xml version="1.0"?>
<!--
Top level element in the group must be a single expression for definition to be valid.
-->
<Site name="SMART PORTAL">
    <Group name="CANADA_PARTNER-PORTAL_USERS_NOT_CONSUMER">
        <Equals attribute="userCountryCode">US</Equals>
        <Not>
            <Equals attribute="userProgramName">Consumer</Equals>
        </Not>
    </Group>

    <Group name="US_PARTNER-PORTAL_USERS_NOT_CONSUMER">
        <Equals attribute="userCountryCode">CA</Equals>
        <Not>
            <Equals attribute="userProgramName">Consumer</Equals>
        </Not>
    </Group>
</Site>
