<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="com.hp.spp.groups.Group" table="SPP_GROUP">
	<id name="id" column="ID" type="long">
	<generator class="sequence">
		<param name="sequence">SPP_GROUP_ID_SEQ</param>
	</generator>
	</id>
	<property name="name" type="string" length="100" column="NAME"/>
			<property name="rulesInternal" type="blob" column="RULES"/>
			<property name="creationDate" type="date" column="CREATIONDATE"/>
			<property name="modificationDate" type="date" column="MODIFICATIONDATE"/>
			<many-to-one name="site" class="com.hp.spp.groups.Site" column="SITE_ID"/>
</class>
</hibernate-mapping>
	