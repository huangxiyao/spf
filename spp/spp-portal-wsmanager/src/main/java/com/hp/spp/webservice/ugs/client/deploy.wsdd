      <parameter name="wsdlPortType" value="UserGroupManager"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getGroups" qname="operNS:getGroups" xmlns:operNS="http://groups.webservice.spp.hp.com" returnQName="getGroupsReturn" returnType="rtns:UserGroupResponse" xmlns:rtns="urn:UserGroupManager" soapAction="" >
        <parameter qname="gRequest" type="tns:UserGroupRequest" xmlns:tns="urn:UserGroupManager"/>
