        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="urn:UserGroupManager"
        qname="ns:UserGroupRequest"
        type="java:com.hp.spp.webservice.ugs.client.UserGroupRequest"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
