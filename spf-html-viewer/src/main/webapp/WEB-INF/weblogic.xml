<?xml version="1.0" encoding="ISO-8859-1"?>
<weblogic-web-app xmlns="http://www.bea.com/ns/weblogic/90">

	<jsp-descriptor>
		<keepgenerated>true</keepgenerated>
		<page-check-seconds>2700</page-check-seconds>
		<debug>false</debug>
	</jsp-descriptor>

	<container-descriptor>
		<servlet-reload-check-secs>2700</servlet-reload-check-secs>
		<resource-reload-check-secs>2700</resource-reload-check-secs>
	</container-descriptor>

	<session-descriptor>
		<timeout-secs>7200</timeout-secs>
		<invalidation-interval-secs>60</invalidation-interval-secs>
		<persistent-store-type>memory</persistent-store-type>
	</session-descriptor>

	<context-root>htmlviewer</context-root>

</weblogic-web-app>