	<build>
		<plugins>
			<plugin>
    			<groupId>com.hp.it.spf.tools</groupId>
    			<artifactId>spf-maven-car-plugin</artifactId>
    			<version>2.1.0</version>
    			<extensions>true</extensions>
    			<configuration>
