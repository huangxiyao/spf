		<artifactId>spf-hpweb-laf</artifactId>
		<version>2.2.0</version>
	</parent>

	<artifactId>CleansheetComponents</artifactId>
	<name>Cleansheet VAP All Components</name>
	<packaging>car</packaging>

	<dependencies>

    	<dependency>
        	<groupId>com.hp.frameworks</groupId>
        	<artifactId>HPLayoutConfigStyleType</artifactId>
			<version>${project.version}</version>
			<type>car</type>
    	</dependency>
    	<dependency>
        	<groupId>com.hp.frameworks</groupId>
        	<artifactId>cleansheet-lib</artifactId>
