	<name>SPF portal components</name>
	<version>1.0.12-SNAPSHOT</version>
	<description>
		All Vignette portal components for the Shared Portal Framework.
		This artifact builds a composite CAR containing the CAR files
		for the other SPF portal components.
	</description>
	<ciManagement>
		<system>Hudson</system>
		<url>
			http://samson.atl.hp.com:8080/hudson/job/spf-portal-components
		</url>
	</ciManagement>
	<scm>
		<connection>scm:svn:https://samson.atl.hp.com/svn/spf/source/trunk/spf-portal-components</connection>
		<developerConnection>scm:svn:https://samson.atl.hp.com/svn/spf/source/trunk/spf-portal-components</developerConnection>
		<url>https://samson.atl.hp.com/svn/spf/source/trunk/spf-portal-components</url>
	</scm>
	<organization>
		<name>Shared Portal Framework: TSG IT</name>
	</organization>
	<build>
		<plugins>
			<plugin>
				<groupId>com.hp.it.spf.tools</groupId>
				<artifactId>spf-maven-car-plugin</artifactId>
				<extensions>true</extensions>
				<configuration>
					<composite>true</composite>
				</configuration>
			</plugin>
		</plugins>
	</build>
