<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig
  PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
  "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

  <settings useStatementNamespaces="true" />

  <typeHandler javaType="java.lang.String" jdbcType="VARCHAR" callback="com.hp.it.cas.xa.ibatis.OracleEmptyStringTypeHandlerCallback" />
    
  <!-- generated maps -->
  <sqlMap resource="com/hp/it/cas/security/dao/USER_ATTR_SqlMap.xml" />
  <sqlMap resource="com/hp/it/cas/security/dao/CMPND_ATTR_SMPL_ATTR_SqlMap.xml" />
  <sqlMap resource="com/hp/it/cas/security/dao/USER_ATTR_VALU_SqlMap.xml" />
  <sqlMap resource="com/hp/it/cas/security/dao/APP_USER_ATTR_PRMSN_SqlMap.xml" />
  <sqlMap resource="com/hp/it/cas/security/dao/APP_CMPND_ATTR_SMPL_ATTR_PRMSN_SqlMap.xml" />
  		
  <!-- custom maps -->
  <sqlMap resource="com/hp/it/cas/security/custom/dao/USER_ATTR_CustomSqlMap.xml" />
  <sqlMap resource="com/hp/it/cas/security/custom/dao/CMPND_ATTR_SMPL_ATTR_CustomSqlMap.xml" />
  <sqlMap resource="com/hp/it/cas/security/custom/dao/USER_ATTR_VALU_CustomSqlMap.xml" />
  <sqlMap resource="com/hp/it/cas/security/custom/dao/APP_CMPND_ATTR_SMPL_ATTR_PRMSN_CustomSqlMap.xml" />
  <sqlMap resource="com/hp/it/cas/security/custom/dao/APP_USER_ATTR_PRMSN_CustomSqlMap.xml" />
  
</sqlMapConfig>
