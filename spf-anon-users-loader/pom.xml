		<version>1.0.1</version>
	</parent>
	<ciManagement>
		<system>Hudson</system>
		<url>http://samson.atl.hp.com:8080/hudson/job/spf-anon-users-loader</url>
	</ciManagement>
	<scm>
		<connection>scm:svn:http://samson.atl.hp.com/svn/spf/source/tools/trunk/spf-anon-users-loader</connection>
		<developerConnection>scm:svn:http://samson.atl.hp.com/svn/spf/source/tools/trunk/spf-anon-users-loader</developerConnection>
		<url>http://samson.atl.hp.com/svn/spf/source/tools/trunk/spf-anon-users-loader</url>
	</scm>
	<organization>
		<name>Shared Portal Framework: TSG IT</name>
	</organization>
    <dependencies>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.5</version>
            <scope>test</scope>
        </dependency>
		<dependency>
			<groupId>vignette</groupId>
			<artifactId>vgn-portal-core</artifactId>
			<version>7.4.2</version>
