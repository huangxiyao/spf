			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.hp.it.spf.sso</groupId>
			<artifactId>spf-portal-session</artifactId>
			<version>1.0.3</version>
			<scope>provided</scope>
		</dependency>
    </dependencies>
