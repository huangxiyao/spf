<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.hp.it.spf.xa</groupId>
		<artifactId>common</artifactId>
		<version>2.3.1</version>
	</parent>

	<groupId>com.hp.it.spf.xa</groupId>
	<artifactId>spf-common-hpp</artifactId>
	<packaging>jar</packaging>
	<name>SPF common HPP WebServices client</name>
	<description>Client API library for HPP WebServices offered by Shared Portal Framework.  This project is used by the SPF portal and portlet frameworks, and is offered to feature-services for their use as well.  This client is current as of the HPP W/S 5.0 "Corralco" release.  Please note: this client is offered on a "best-effort" basis only.  If you use this client, you should understand that the HPP (not SPF) team owns these Web Services, and that this client will not necessarily be kept up-to-date with all subsequent HPP W/S releases and features (only those needed by the framework).</description>
	<inceptionYear>December 2008</inceptionYear>


	<dependencies>
		<dependency>
			<groupId>castor</groupId>
			<artifactId>castor</artifactId>
		</dependency>
		<dependency>
			<groupId>org.apache.axis</groupId>
			<artifactId>axis-jaxrpc</artifactId>
		</dependency>
		<dependency>
			<groupId>org.apache.axis</groupId>
			<artifactId>axis</artifactId>
		</dependency>
		<dependency>
			<groupId>org.apache.axis</groupId>
			<artifactId>axis-saaj</artifactId>
		</dependency>
		<dependency>
			<groupId>dnsjava</groupId>
			<artifactId>dnsjava</artifactId>
		</dependency>
		<dependency>
			<groupId>commons-discovery</groupId>
			<artifactId>commons-discovery</artifactId>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>xerces</groupId>
			<artifactId>xercesImpl</artifactId>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>com.hp.it.spf.xa</groupId>
			<artifactId>spf-common-utilities</artifactId>
			<version>${project.version}</version>
		</dependency>
	</dependencies>
</project>
